{
  "env": {
    "browser": true,
    "es2022": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "max-depth": ["error", 2],
    "no-param-reassign": "off",
    "no-use-before-define": "off",
    "consistent-return": "off",
  },
}
